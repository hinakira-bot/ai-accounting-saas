{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "style.css",
            "use": "@vercel/static"
        },
        {
            "src": "script.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        { "src": "/api/(.*)", "dest": "main.py" },
        { "src": "/style.css(.*)", "dest": "style.css" },
        { "src": "/script.js(.*)", "dest": "script.js" },
        { "src": "/(.*)", "dest": "index.html" }
    ]
}
